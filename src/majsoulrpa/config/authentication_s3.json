{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "required": [
        "method",
        "email_address",
        "bucket_name",
        "key_prefix"
    ],
    "properties": {
        "method": {
            "const": "s3"
        },
        "email_address": {
            "type": "string"
        },
        "bucket_name": {
            "type": "string"
        },
        "key_prefix": {
            "type": "string"
        }
    },
    "additionalProperties": {
        "aws_profile": {
            "type": "string"
        }
    }
}